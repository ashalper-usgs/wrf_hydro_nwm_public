# Makefile.in

.PHONY: all clean

CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
DEFS = @DEFS@

MPIFC = @MPIFC@
FCFLAGS = @FCFLAGS@

objects = module_lis_HYDRO.o lis_drv_HYDRO.o

all: $(objects)

$(objects): %.o: %.f
	$(MPIFC) -o $@ $(FCFLAGS) $<

%.f: %.F
	$(CPP) $(CPPFLAGS) $(DEFS) $< > $@

clean:
	rm -f *.o *.mod *.stb *~ *.f
