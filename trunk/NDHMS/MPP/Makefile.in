# Makefile.in

.PHONY: all clean

CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
DEFS = @DEFS@

MPIFC = @MPIFC@
FCFLAGS = @FCFLAGS@

objects = CPL_WRF.o mpp_land.o module_mpp_ReachLS.o	\
          module_mpp_GWBUCKET.o

all: $(objects)

$(objects): %.o: %.f
	$(MPIFC) -o $@ $(FCFLAGS) $<

%.f: %.F
	$(CPP) $(CPPFLAGS) $(DEFS) $< > $@

clean:
	rm -f *.o *.mod *.f *.stb *~
