# Makefile.in

.PHONY: all clean

CPP = @CPP@
CPPFLAGS = @CPPFLAGS@
DEFS = @DEFS@

MPIFC = @MPIFC@
FCFLAGS = @FCFLAGS@

include ../global.mk

all: module_HYDRO_drv.o

module_HYDRO_drv.o: %.o: %.f
	$(MPIFC) -o $@ $(FCFLAGS) $(addprefix -I../,Routing MPP Data_Rec) $<

%.f: %.F
	$(CPP) $(CPPFLAGS) $(DEFS) $< > $@

clean:
	rm -f *.o *.mod *.stb *~ *.f
